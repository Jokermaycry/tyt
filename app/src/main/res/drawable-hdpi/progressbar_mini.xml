<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    
    
    <!-- 改变了下android:endColor="#F5F5F5" android:startColor="#BEBEBE" 的色值  #D26900    android:endColor="#165CBC"
                    android:startColor="#85B0E9" -->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:angle="270"
                android:centerY="0.75"
                android:endColor="#c5c6c9" 
                android:startColor="#c5c6c9" /><!-- 白色 -->
        </shape>
    </item>
 
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient
                    android:angle="270"
                    android:centerY="0.75"
                    android:endColor="#c5c6c9"
                    android:startColor="#c5c6c9" />
            </shape>
        </clip>
    </item>
 
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerY="0.75"
                    android:endColor="#2a6de7"
                    android:startColor="#2a6de7" /><!-- 蓝色 -->
            </shape>
        </clip>
    </item>
</layer-list>
